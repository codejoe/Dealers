<div class="container">
  <h1>Home#Index</h1>
  <p>
  <% if current_user %>
    User: <%= current_user.email %>. <%= link_to 'Sign Out!', destroy_user_session_path, method: 'delete' %> 
  <% else %>
    Please <%= link_to 'Sign In', new_user_session_path %>. Don't have an account? <%= link_to 'Sign Up', new_user_registration_path %>. Forgot Password? <%= link_to 'Click Here', new_user_password_path %>
  <% end %>
  </p>
</div>